
<div class="pull-right">
<% if signed_in? %>

<p class="welcome">Welcome, <%= current_user.name %>!</p>

	<%= link_to 'Sign out', signout_path, method: "delete", class: "btn btn-primary header-bt" %><br>

<% else %>

	<%= link_to 'Sign in', signin_path, class: "btn btn-primary header-bt" %><br>

<% end %>
</div>